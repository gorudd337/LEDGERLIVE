{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [":dependencyDashboard", ":ignoreModulesAndTests"],
  "dependencyDashboard": true,
  "labels": ["dependencies"],
  "automerge": false,
  "detectGlobalManagerConfig": true,
  "ignoreScripts": false,
  "vulnerabilityAlerts": {
    "enabled": true,
    "labels": ["security"],
    "prPriority": 999
  },
  "packageRules": [],
  "prConcurrentLimit": 8,
  "separateMinorPatch": true,
  "timezone": "Europe/Paris",
  "enabledManagers": ["npm"]
}
